<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:forceDarkAllowed="false"
    android:theme="@android:style/Theme.Light"
    tools:context=".MainActivity">

    <Spinner
        android:id="@+id/tipoU"
        android:layout_width="185dp"
        android:layout_height="43dp"
        android:layout_marginStart="215dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="338dp"
        android:layout_marginBottom="301dp"
        android:entries="@array/tipoU"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="215dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="418dp"
        android:layout_marginBottom="342dp"
        android:text="@string/tipousert"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/usuario"
        android:layout_width="290dp"
        android:layout_height="47dp"
        android:layout_marginStart="215dp"
        android:layout_marginTop="159dp"
        android:layout_marginEnd="226dp"
        android:layout_marginBottom="205dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/contrasena"
        android:layout_width="292dp"
        android:layout_height="45dp"
        android:layout_marginStart="215dp"
        android:layout_marginTop="255dp"
        android:layout_marginEnd="224dp"
        android:layout_marginBottom="111dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="215dp"
        android:layout_marginTop="125dp"
        android:layout_marginEnd="465dp"
        android:layout_marginBottom="267dp"
        android:text="@string/usuariot"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="215dp"
        android:layout_marginTop="221dp"
        android:layout_marginEnd="439dp"
        android:layout_marginBottom="171dp"
        android:text="@string/contrasenat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="421dp"
        android:layout_marginTop="313dp"
        android:layout_marginEnd="222dp"
        android:layout_marginBottom="50dp"
        android:onClick="logueo"
        android:text="@string/botonEntrar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>